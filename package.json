{
  "name": "bradford-informed-guidance",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "prerender": "wait-on tcp:8080 && node scripts/prerender-simple.mjs",
    "prerender:all": "concurrently -k -n PREVIEW,PRERENDER -c auto \"npm:preview\" \"npm:prerender\"",
    "tunnel": "node scripts/tunnel.js",
    "live": "cross-env NGROK_DOMAIN=bradfordinformedguidance.ngrok-free.app concurrently -k -n VITE,NGROK -c auto \"npm:dev\" \"npm:tunnel\"",
    "dev:vercel": "vercel dev",
    "postbuild": "node scripts/generate-sitemaps.mjs && node scripts/generate-route-htmls.cjs",
    "build:full": "node scripts/build-full.mjs", 
    "build:vercel": "npm run build",
    "verify:security": "pwsh -File scripts/verify-security.ps1",
    "check:headers": "pwsh -File scripts/remote-header-checks.ps1 -BaseUrl https://bradfordinformedguidance.com",
    "check:headers:win": "powershell -ExecutionPolicy Bypass -File scripts\\remote-header-checks.ps1 -BaseUrl https://bradfordinformedguidance.com",
    "ringy:e2e": "pwsh -File scripts/ringy-e2e-check.ps1 -Host https://bradfordinformedguidance.com -Label TEST LEAD",
    "verify:seo": "pwsh -File scripts/verify-seo.ps1",
    "verify:seo:win": "powershell -ExecutionPolicy Bypass -File scripts\\verify-seo.ps1",
    "verify:sitemap": "node scripts/verify-sitemap.mjs"
  },
  "dependencies": {
    "@fontsource/playfair-display": "^5.2.6",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vercel/node": "^5.3.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "input-otp": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "lovable-tagger": "^1.1.9",
    "lucide-react": "^0.539.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "react-router-dom": "^6.22.3",
    "react-type-animation": "^3.2.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@testing-library/react": "^15.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "d3-geo": "^3.1.1",
    "d3-geo-projection": "^4.0.0",
    "postcss": "^8.4.24",
    "puppeteer": "^21.11.0",
    "tailwindcss": "^3.4.1",
    "vitest": "^1.6.0",
    "topojson-client": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.19",
    "wait-on": "^8.0.4",
    "jsdom": "^24.0.0"
  },
  "engines": {
    "node": ">=18.17"
  }
}
